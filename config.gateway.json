{
        "firewall": {
                "name": {
                        "IPTV_LOCAL": {
                                "description": "WAN IPTV to router",
                                "default-action": "accept",
                                "rule": {
                                        "10": {
                                                "action": "accept",
                                                "description": "Allow established/related",
                                                "state": {
                                                        "established": "enabled",
                                                        "related": "enabled"
                                                }
                                        }
                                        
                                }
                        }
        }
},
        "interfaces": {
                "ethernet": {
                        "eth0": {
                                "firewall": {
                                        "in": {
                                                "name": "WAN_IN"
                                        },
                                        "local": {
                                                "name": "WAN_LOCAL"
                                        },
                                        "out": {
                                                "name": "WAN_OUT"
                                        }
                                },
                                "description": "WAN",
                                "mac": "bc:99:11:0c:2d:02",
                                "dhcp-options": {
                                        "default-route": "no-update",
                                        "name-server": "no-update"
                                },
                                "vif": {
                                        "4": {
                                                "address": [
                                                        "dhcp"
                                                ],
                                                "description": "Online_IPTV",
                                                "mac": "bc:99:11:0c:2d:03",
                                                "dhcp-options": {
                                                        "client-option": [
                                                                "send vendor-class-identifier &quot;dlsforum.org&quot;;"
                                                        ]
                                                },
                                                "ip": {
                                                        "source-validation": "loose"
                                                }
                                        }
                                }
                        }
                }
        },
        "protocols": {
                "igmp-proxy": {
                        "interface": {
                                "eth0.4": {
